{
    "name": "registrationForm",
    "useSegment": "members",
    "elements": [
        {
            "type": "field",
            "object": "membershipType",
            "default": "IND",
            "triggers": [
                {
                    "value": ["IND", "STU"],
                    "actions": [
                        {
                            "change": ["enable", "display"],
                            "target": ["family", "business"],
                            "value": false
                        }
                    ]
                },
                {
                    "value": "FAM",
                    "actions": [
                        {
                            "change": ["enable", "display"],
                            "target": "family",
                            "value": true
                        },
                        {
                            "change": ["enable", "display"],
                            "target": "business",
                            "value": false
                        }
                    ]
                },
                {
                    "value": ["BUS", "FEL"],
                    "actions": [
                        {
                            "change": ["enable", "display"],
                            "target": "family",
                            "value": false
                        },
                        {
                            "change": ["enable", "display"],
                            "target": "business",
                            "value": true
                        }
                    ]
                },
                {
                    "value": ["PAT", "SPO"],
                    "actions": [
                        {
                            "change": ["enable", "display"],
                            "target": "family",
                            "value": false
                        },
                        {
                            "change": ["enable", "display"],
                            "target": "business",
                            "value": true
                        }
                    ]
                },
                {
                    "event": "onchange",
                    "value": null,
                    "actions": [
                        {
                            "change": "script",
                            "value": "form.membershipFee.validation.minValue=this.sidecar.fee.minValue;"
                        }
                    ]
                }
            ]
        },
        {
            "type": "section",
            "labels": {
                "heading": "Primary Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": ["primaryFirstName", "primaryLastName"]
                },
                "primaryEmail",
                "primaryPublicName"
            ]
        },
        {
            "type": "section",
            "memberOf": "family",
            "labels": {
                "heading": "Second Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": ["secondaryFirstName", "secondaryLastName"]
                },
                "secondaryPublicName:family"
            ]
        },
        "businessName:business",
        {
            "type": "button",
            "function": "submit",
            "labels": {
                "inner": "Join"
            },
            "show": "purpose:primary"
        }
    ]
}

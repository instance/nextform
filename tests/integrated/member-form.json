{
    "name": "registrationForm",
    "useSegment": "members",
    "elements": [
        {
            "type": "field",
            "object": "membershipLevel",
            "default": "IND",
            "triggers": [
                {
                    "event": "change",
                    "value": ["IND", "STU"],
                    "actions": [
                        "{family},{business}:display:false",
                        "{business-enable}:enable:false",
                        "{person-enable}:enable:true"
                    ]
                },
                {
                    "event": "change",
                    "value": "FAM",
                    "actions": [
                        {
                            "subject": "display",
                            "target": "{family}",
                            "value": true
                        },
                        {
                            "subject": "display",
                            "target": "{business}",
                            "value": false
                        },
                        {
                            "subject": "enable",
                            "target": "{business-enable}",
                            "value": false
                        },
                        "{person-enable}:enable:true"
                    ]
                },
                {
                    "event": "change",
                    "value": ["BUS", "FEL"],
                    "actions": [
                        {
                            "subject": "display",
                            "target": "{family}",
                            "value": false
                        },
                        {
                            "subject": "display",
                            "target": "{business}",
                            "value": true
                        },
                        {
                            "subject": "enable",
                            "target": "{business-enable}",
                            "value": true
                        },
                        "{person-enable}:enable:false"
                    ]
                },
                {
                    "event": "change",
                    "value": ["PAT", "SPO"],
                    "actions": [
                        {
                            "subject": "display",
                            "target": "{family}",
                            "value": false
                        },
                        {
                            "subject": "display",
                            "target": "{business}",
                            "value": true
                        },
                        {
                            "subject": "enable",
                            "target": "{business-enable}",
                            "value": true
                        },
                        "{person-enable}:enable:true"
                    ]
                },
                {
                    "event": "change",
                    "value": null,
                    "actions": [
                        {
                            "subject": "script",
                            "value": "form.membershipFee.validation.minValue=this.sidecar.fee.minValue;"
                        }
                    ]
                }
            ]
        },
        {
            "type": "section",
            "labels": {
                "heading": "Primary Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": ["primaryFirstName", "primaryLastName"]
                },
                "primaryEmail",
                "primaryPublicName",
                "primaryMemberType"
            ]
        },
        {
            "type": "section",
            "memberOf": "family",
            "labels": {
                "heading": "Second Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": ["secondaryFirstName", "secondaryLastName"]
                },
                "secondaryPublicName",
                "secondaryMemberType"
            ]
        },
        {
            "type": "field",
            "id": "foo22",
            "object": "businessName",
            "memberOf": "business",
            "display": false
        },
        {
            "type": "button",
            "function": "submit",
            "labels": {
                "inner": "Join"
            },
            "show": "purpose:primary"
        }
    ]
}

{
    "name": "registrationForm",
    "useSegment": "members",
    "elements": [
        {
            "type": "field",
            "object": "membershipLevel",
            "default": "IND",
            "triggers": [
                {
                    "event": "change",
                    "value": [
                        "IND",
                        "STU",
                        "FAM"
                    ],
                    "actions": [
                        "{business-enable}:enable:false",
                        "{business-enable}:checked:false",
                        "{business}:display:false",
                        "{person-enable}:enable:true",
                        "scratch_feeSelect:display:false"
                    ]
                },
                {
                    "event": "change",
                    "value": [
                        "IND",
                        "STU"
                    ],
                    "actions": [
                        "{family}:display:false"
                    ]
                },
                {
                    "event": "change",
                    "value": "FAM",
                    "actions": [
                        "{family}:display:true"
                    ]
                },
                {
                    "event": "change",
                    "value": [
                        "BUS",
                        "FEL"
                    ],
                    "actions": [
                        "{family}:display:false",
                        "{business}:display:true",
                        "{business-enable}:enable:true",
                        "{person-enable}:enable:false",
                        "{person-enable}:checked:false",
                        "scratch_feeSelect:display:false"
                    ]
                },
                {
                    "event": "change",
                    "value": [
                        "PAT",
                        "SPO"
                    ],
                    "actions": [
                        "{family}:display:false",
                        "{business}:display:true",
                        "{business-enable}:enable:true",
                        "{person-enable}:enable:true",
                        "scratch_feeSelect:display:true"
                    ]
                },
                {
                    "event": "change",
                    "actions": [
                        {
                            "subject": "script",
                            "value": "setFeeAttributes(this);"
                        }
                    ]
                }
            ]
        },
        {
            "type": "section",
            "labels": "Primary Member",
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        "primaryFirstName",
                        "primaryLastName"
                    ]
                },
                "primaryEmail",
                "primaryPhone",
                "primaryPublicName",
                {
                    "type": "field",
                    "object": "primaryMemberType",
                    "triggers": [
                        {
                            "event": "change",
                            "value": "VA",
                            "actions": [
                                "{firstVA}:display:checked"
                            ]
                        },
                        {
                            "event": "change",
                            "value": "XX",
                            "actions": [
                                "{firstXX}:display:checked"
                            ]
                        }
                    ]
                },
                {
                    "type": "field",
                    "object": "primaryTypeOther",
                    "memberOf": "firstXX",
                    "display": false
                },
                {
                    "type": "field",
                    "object": "primaryVisualArtsMedia",
                    "memberOf": "firstVA",
                    "display": false,
                    "show": "optionwidth:3"
                },
                {
                    "type": "field",
                    "object": "primaryVisualArtsMediaOther",
                    "memberOf": "firstVA",
                    "display": false
                }
            ]
        },
        {
            "type": "section",
            "memberOf": "family",
            "labels": "Second Member",
            "display": false,
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        "secondaryFirstName",
                        "secondaryLastName"
                    ]
                },
                "secondaryPublicName",
                {
                    "type": "field",
                    "object": "secondaryMemberType",
                    "triggers": [
                        {
                            "event": "change",
                            "value": "VA",
                            "actions": [
                                "{secondVA}:display:checked"
                            ]
                        },
                        {
                            "event": "change",
                            "value": "XX",
                            "actions": [
                                "{secondXX}:display:checked"
                            ]
                        }
                    ]
                },
                {
                    "type": "field",
                    "object": "secondaryTypeOther",
                    "memberOf": "secondXX",
                    "display": false
                },
                {
                    "type": "field",
                    "object": "secondaryVisualArtsMedia",
                    "memberOf": "secondVA",
                    "display": false,
                    "show": "optionwidth:3"
                },
                {
                    "type": "field",
                    "object": "secondaryVisualArtsMediaOther",
                    "memberOf": "secondVA",
                    "display": false
                }
            ]
        },
        {
            "type": "field",
            "object": "businessName",
            "memberOf": "business",
            "display": false
        },
        {
            "type": "field",
            "object": "scratch\/feeSelect",
            "labels": {
                "help": "."
            },
            "display": false,
            "triggers": [
                {
                    "event": "change",
                    "actions": [
                        {
                            "subject": "script",
                            "value": "updateFee(this);"
                        }
                    ]
                }
            ]
        },
        {
            "type": "field",
            "object": "membershipFee",
            "enabled": false,
            "default": 30
        },
        {
            "type": "button",
            "function": "submit",
            "labels": {
                "inner": "Join"
            },
            "show": "purpose:primary"
        }
    ]
}
{
    "name": "registrationForm",
    "useSegment": "members",
    "elements": [
        {
            "type": "field",
            "object": "membershipType",
            "default": "IND",
            "triggers": [
                {
                    "value": [
                        "IND",
                        "STU"
                    ],
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "visible"
                            ],
                            "target": [
                                "family",
                                "business"
                            ],
                            "value": false
                        }
                    ]
                },
                {
                    "value": "FAM",
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "visible"
                            ],
                            "target": "family",
                            "value": true
                        },
                        {
                            "change": [
                                "enable",
                                "visible"
                            ],
                            "target": "business",
                            "value": false
                        }
                    ]
                },
                {
                    "value": [
                        "BUS",
                        "FEL"
                    ],
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "visible"
                            ],
                            "target": "family",
                            "value": false
                        },
                        {
                            "change": [
                                "enable",
                                "visible"
                            ],
                            "target": "business",
                            "value": true
                        }
                    ]
                },
                {
                    "value": [
                        "PAT",
                        "SPO"
                    ],
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "visible"
                            ],
                            "target": "family",
                            "value": false
                        },
                        {
                            "change": [
                                "enable",
                                "visible"
                            ],
                            "target": "business",
                            "value": true
                        }
                    ]
                },
                {
                    "event": "onchange",
                    "value": null,
                    "actions": [
                        {
                            "change": "script",
                            "value": "form.membershipFee.validation.minValue=this.sidecar.fee.minValue;"
                        }
                    ]
                }
            ]
        },
        {
            "type": "section",
            "labels": {
                "heading": "Primary Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        {
                            "type": "field",
                            "object": "primaryFirstName"
                        },
                        {
                            "type": "field",
                            "object": "primaryLastName"
                        }
                    ]
                },
                {
                    "type": "field",
                    "object": "primaryEmail"
                },
                {
                    "type": "field",
                    "object": "primaryPublicName"
                }
            ]
        },
        {
            "type": "section",
            "memberOf": "family",
            "labels": {
                "heading": "Second Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        {
                            "type": "field",
                            "object": "secondaryFirstName"
                        },
                        {
                            "type": "field",
                            "object": "secondaryLastName"
                        }
                    ]
                },
                {
                    "type": "field",
                    "memberOf": "family",
                    "object": "secondaryPublicName"
                }
            ]
        },
        {
            "type": "field",
            "memberOf": "business",
            "object": "businessName"
        },
        {
            "type": "button",
            "labels": {
                "inner": "Join"
            },
            "function": "submit"
        }
    ]
}
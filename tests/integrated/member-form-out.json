{
    "name": "registrationForm",
    "useSegment": "members",
    "elements": [
        {
            "type": "field",
            "object": "membershipLevel",
            "default": "IND",
            "triggers": [
                {
                    "event": "change",
                    "value": [
                        "IND",
                        "STU"
                    ],
                    "actions": [
                        "{family},{business}:display:0",
                        "{business-enable}:enable:0",
                        "{person-enable}:enable:1"
                    ]
                },
                {
                    "event": "change",
                    "value": "FAM",
                    "actions": [
                        "{family}:display:1",
                        "{business}:display:0",
                        "{business-enable}:enable:0",
                        "{person-enable}:enable:1"
                    ]
                },
                {
                    "event": "change",
                    "value": [
                        "BUS",
                        "FEL"
                    ],
                    "actions": [
                        "{family}:display:0",
                        "{business}:display:1",
                        "{business-enable}:enable:1",
                        "{person-enable}:enable:0"
                    ]
                },
                {
                    "event": "change",
                    "value": [
                        "PAT",
                        "SPO"
                    ],
                    "actions": [
                        "{family}:display:0",
                        "{business}:display:1",
                        "{business-enable}:enable:1",
                        "{person-enable}:enable:1"
                    ]
                },
                {
                    "event": "change",
                    "actions": [
                        {
                            "subject": "script",
                            "value": "form.membershipFee.validation.minValue=this.sidecar.fee.minValue;"
                        }
                    ]
                }
            ]
        },
        {
            "type": "section",
            "labels": {
                "heading": "Primary Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        "primaryFirstName",
                        "primaryLastName"
                    ]
                },
                "primaryEmail",
                "primaryPublicName",
                "primaryMemberType"
            ]
        },
        {
            "type": "section",
            "memberOf": "family",
            "labels": {
                "heading": "Second Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        "secondaryFirstName",
                        "secondaryLastName"
                    ]
                },
                "secondaryPublicName",
                "secondaryMemberType"
            ]
        },
        {
            "type": "field",
            "id": "foo22",
            "object": "businessName",
            "memberOf": "business",
            "display": false
        },
        {
            "type": "button",
            "function": "submit",
            "labels": {
                "inner": "Join"
            },
            "show": "purpose:primary"
        }
    ]
}
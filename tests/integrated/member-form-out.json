{
    "name": "registrationForm",
    "useSegment": "members",
    "elements": [
        {
            "type": "field",
            "object": "membershipType",
            "default": "IND",
            "triggers": [
                {
                    "value": [
                        "IND",
                        "STU"
                    ],
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "display"
                            ],
                            "value": false,
                            "target": [
                                "family",
                                "business"
                            ]
                        }
                    ]
                },
                {
                    "value": "FAM",
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "display"
                            ],
                            "value": true,
                            "target": "family"
                        },
                        {
                            "change": [
                                "enable",
                                "display"
                            ],
                            "value": false,
                            "target": "business"
                        }
                    ]
                },
                {
                    "value": [
                        "BUS",
                        "FEL"
                    ],
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "display"
                            ],
                            "value": false,
                            "target": "family"
                        },
                        {
                            "change": [
                                "enable",
                                "display"
                            ],
                            "value": true,
                            "target": "business"
                        }
                    ]
                },
                {
                    "value": [
                        "PAT",
                        "SPO"
                    ],
                    "actions": [
                        {
                            "change": [
                                "enable",
                                "display"
                            ],
                            "value": false,
                            "target": "family"
                        },
                        {
                            "change": [
                                "enable",
                                "display"
                            ],
                            "value": true,
                            "target": "business"
                        }
                    ]
                },
                {
                    "event": "onchange",
                    "actions": [
                        {
                            "change": "script",
                            "value": "form.membershipFee.validation.minValue=this.sidecar.fee.minValue;"
                        }
                    ]
                }
            ]
        },
        {
            "type": "section",
            "labels": {
                "heading": "Primary Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        "primaryFirstName",
                        "primaryLastName"
                    ]
                },
                "primaryEmail",
                "primaryPublicName"
            ]
        },
        {
            "type": "section",
            "memberOf": "family",
            "labels": {
                "heading": "Second Member"
            },
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        "secondaryFirstName",
                        "secondaryLastName"
                    ]
                },
                "secondaryPublicName:family"
            ]
        },
        "businessName:business",
        {
            "type": "button",
            "labels": {
                "inner": "Join"
            },
            "show": "purpose:primary",
            "function": "submit"
        }
    ]
}
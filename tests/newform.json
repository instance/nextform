                }
            ]
        },
        {
            "type": "section",
            "elements": [
                {
                    "type": "cell",
                    "elements": [
                        {
                            "type": "field",
                            "object": "user2-areaCode"
                        },
                        {
                            "type": "field",
                            "object": "user2-phoneNumber"
                        },
                        {
                            "type": "field",
                            "object": "user2-phoneExtension"
                        }
                    ]
